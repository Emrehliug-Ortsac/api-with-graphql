type Compra{
    id:ID!
    quantidade: Int!
    status:String!
    cliente:Cliente
    produto:Produto!
}

input CompraInput{
    id:ID
    quantidade: Int!
    status:String
    clienteId:ID!
    produtoId:ID!
}

extend type Mutation {
    saveCompra(compra:CompraInput!):Compra!
    deleteCompra(id:ID!):Boolean!
}